    <div class="col-md-12 jumbotron" id="Jumbo">
        <div class="text-center">
            <h1> Mongo Scraper</h1>
            <h3>New York Times Edition</h3>
        </div>

    </div>
    <div class="col-md-12">
        {{#if article}}
            {{#each article}}
            <div class="panel panel-default">
                <div class="panel-heading">
                <button id= "svbtn" type="button" class="btn btn-primary save" data-id={{this._id}}>Save Article</button>
                    <h4>{{this.title}}</h4>
                    <p> {{this.summary}}</p>
                    <a href={{this.link}} target="_blank">{{this.link}}</a>
                
                
                </div>
                
            </div>
            {{/each}}
        {{else}}
        <div class="panel panel-default">
            <div class="panel-body">
            <h4>Uh Oh. Looks like we don't have any new articles.</h4>
            </div>
         </div>
        {{/if}}
    </div>
